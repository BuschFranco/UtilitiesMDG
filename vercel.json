{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "astro",
  "rewrites": [
    {
      "source": "/",
      "destination": "/es/"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/es/",
      "permanent": true
    }
  ]
}